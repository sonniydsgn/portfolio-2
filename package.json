{
	"name": "astro-starter",
	"type": "module",
	"version": "1.0.0",
	"browserslist": {
		"production": [
			"Chrome >= 109",
			"Edge >= 109",
			"Firefox >= 115",
			"Safari >= 15.4"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"dev": "astro dev",
		"dev:host": "astro dev --host",
		"build": "astro build",
		"preview": "astro preview",
		"preview:host": "astro preview --host",
		"deploy": "vercel --prod",
		"test": "node-w3c-validator -f lint -evH -i dist/**/*.html"
	},
	"dependencies": {
		"@astrojs/sitemap": "^3.2.1",
		"astro": "^4.16.7",
		"astro-icon": "^1.1.1",
		"typescript": "^5.6.3"
	},
	"devDependencies": {
		"@floating-ui/dom": "^1.6.11",
		"@typescript-eslint/parser": "^8.11.0",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-astro": "^1.3.0",
		"eslint-plugin-prettier": "^5.2.1",
		"globals": "^15.11.0",
		"lightningcss": "^1.27.0",
		"prettier": "^3.3.3",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-astro-organize-imports": "^0.4.10",
		"prettier-plugin-css-order": "^2.1.2",
		"prettier-plugin-organize-imports": "^4.1.0",
		"svgo": "^3.3.2",
		"transliteration": "^2.3.5",
		"typograf": "^7.4.1"
	}
}
