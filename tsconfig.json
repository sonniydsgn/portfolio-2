{
	"extends": "astro/tsconfigs/base",
	"types": ["astro/client"],
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": ".",
		"outDir": "dist",
		"noEmit": true,
		"typeRoots": ["./types", "./node_modules/@types"],
		"paths": {
			"@/*": ["src/*"]
		}
	},
	"exclude": ["node_modules", "dist"],
	"include": ["src"]
}
